{
  "name": "@finsys/core",
  "version": "1.0.1",
  "description": "Convert unified form configurations into Zod schemas, React Hook Form defaults, and SurveyJS JSON",
  "repository": {
    "type": "git",
    "url": "https://github.com/ExtraGalaxies/finsys-core.git"
  },
  "homepage": "https://github.com/ExtraGalaxies/finsys-core#readme",
  "bugs": {
    "url": "https://github.com/ExtraGalaxies/finsys-core/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./schema": {
      "import": "./dist/schema/unified-form.schema.json"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc && cp src/schema/unified-form.schema.json dist/schema/unified-form.schema.json",
    "dev": "tsc --watch",
    "test": "vitest run",
    "lint": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "survey-core": "^2.5.0"
  },
  "peerDependenciesMeta": {
    "survey-core": {
      "optional": true
    }
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/node": "^25.0.10",
    "survey-core": "^2.5.6",
    "typescript": "^5.4.0",
    "vitest": "^4.0.18"
  },
  "keywords": [
    "form-generator",
    "zod",
    "react-hook-form",
    "surveyjs",
    "form-config",
    "multi-step-form",
    "form-validation",
    "json-schema",
    "finsys"
  ],
  "license": "Apache-2.0",
  "engines": {
    "node": ">=18"
  }
}
